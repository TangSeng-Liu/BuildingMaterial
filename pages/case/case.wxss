/* pages/case/case.wxss */
.acreage{ justify-content:space-between;align-items:center; padding: 5px 50rpx; padding-bottom: 10px; margin-top: 10px;}
.acreage button{ line-height: 2;}
.acreage picker{border: 1px solid #f84056; border-radius: 5px; padding: 0px 0px 0px 10px;font-size: .8em;line-height: 2; justify-content: space-between;overflow: hidden;}
.arrow{ background-color: #f84056;color: #fff; margin-left: 10px;border-radius: 4px;font-weight: bold;text-align: center; font-size: 1.5em; line-height: 1.4em; padding:0 15rpx}
.acreage button.active{background-color: #f84056;color: #fff}
.case_bolck>view:nth-child(2){ font-size: .8rem; color: #252525}
.case_bolck image{height: 210px; }
.case>navigator{
  animation-name: moveToTop; /*动画名称*/
  animation-duration: 1s; /*动画持续时间*/
  animation-delay: 0s; /*延迟时间*/
}